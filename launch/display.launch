<launch>
  <arg
    name="model" />
  <param
    name="robot_description"
    command="$(find xacro)/xacro '$(find osumy_description)/urdf/osumy.urdf.xacro'" />

  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find osumy_description)/config/urdf.rviz" />
</launch>
